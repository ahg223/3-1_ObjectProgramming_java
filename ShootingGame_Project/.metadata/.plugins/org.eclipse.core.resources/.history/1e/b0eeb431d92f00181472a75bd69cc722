import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int[] integer= {0,0,0};
		
		System.out.println("Write three integer in.");
		
		for(int i=0;i<3;i++) {
			System.out.println("int ["+i+"]= ");
			integer[i]=scan.nextInt();
			
		}
		

	}

}
